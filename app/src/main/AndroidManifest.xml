<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:name=".ui.module.main.MainApplication"
        android:icon="@drawable/resalon_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/resalon_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ReservationDemo"
        tools:targetApi="31" >
        <activity
            android:name=".ui.module.auth.LoginActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".ui.module.home.MainUserActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".ui.module.feed.HomeFeedActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".ui.module.company.CompanyDetailActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
</manifest>